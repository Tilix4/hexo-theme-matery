<% if (theme.socialLink.email) { %>
    <a href="mailto:<%= theme.socialLink.email %>" class="tooltipped" target="_blank" data-tooltip="email" data-position="top" data-delay="50">
        <i class="fa fa-envelope-open"></i>
    </a>
<% } %>

<% if (theme.socialLink.linkedin) { %>
    <a href="<%= theme.socialLink.linkedin %>" class="tooltipped" target="_blank" data-tooltip="LinkedIn" data-position="top" data-delay="50">
        <i class="fa fa-linkedin"></i>
    </a>
<% } %>

<% if (theme.socialLink.github) { %>
    <a href="<%= theme.socialLink.github %>" class="tooltipped" target="_blank" data-tooltip="GitHub" data-position="top" data-delay="50">
        <i class="fa fa-github"></i>
    </a>
<% } %>

<% if (theme.socialLink.gitlab) { %>
    <a href="<%= theme.socialLink.gitlab %>" class="tooltipped" target="_blank" data-tooltip="Gitlab" data-position="top" data-delay="50">
        <i class="fab fa-gitlab"></i>
    </a>
<% } %>

<% if (config.feed && config.feed.path == 'atom.xml') { %>
    <a href="<%- url_for('/atom.xml') %>" class="tooltipped" target="_blank" data-tooltip="RSS" data-position="top" data-delay="50">
        <i class="fa fa-rss"></i>
    </a>
<% } %>
